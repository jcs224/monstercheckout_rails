<% if @user == current_user %> 
	<% provide(:title, 'Profile') %> 
<% else %> 
	<% provide(:title, @user.name) %>
<% end %>
<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
